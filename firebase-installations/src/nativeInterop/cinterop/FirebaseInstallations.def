# based on https://github.com/firebase/firebase-ios-sdk/blob/v8.15.0/FirebaseInstallations.podspec
language = Objective-C
package = suntrix.kmp.firebase.installations
modules = FirebaseInstallations
compilerOpts = -framework FirebaseCore \
               -framework FirebaseInstallations \
               -framework GoogleUtilities \
               -framework PromisesObjC \
               -framework Security
linkerOpts = -framework FirebaseCore \
             -framework FirebaseInstallations \
             -framework GoogleUtilities \
             -framework PromisesObjC \
             -framework Security