# https://github.com/firebase/firebase-ios-sdk/blob/v8.15.0/FirebaseCore.podspec
# https://github.com/firebase/firebase-ios-sdk/blob/v8.15.0/FirebaseCoreDiagnostics.podspec
language = Objective-C
package = suntrix.kmp.firebase.core
modules = FirebaseCore
compilerOpts = -framework FirebaseCore \
               -framework FirebaseCoreDiagnostics \
               -framework GoogleDataTransport \
               -framework GoogleUtilities \
               -framework nanopb
linkerOpts = -framework FirebaseCore \
             -framework FirebaseCoreDiagnostics \
             -framework GoogleDataTransport \
             -framework GoogleUtilities \
             -framework nanopb \
             -framework Foundation
linkerOpts.ios = -framework UIKit
linkerOpts.osx = -framework AppKit
linkerOpts.tvos = -framework UIKit